    "import re\n",
    "values = []\n",
    "with open(\"input.txt\") as file:\n",
    "    for line in file:\n",
    "        values.append(line.rstrip())\n",
    "sum = 0\n",
    "for line in values:\n",
    "    digits_only = re.sub('\\D','',line)\n",
    "    sum += int(digits_only[0] + digits_only[-1])\n",
    "print(sum)"
   ]
